  "author": "Tom Shawver <tom@frosteddesign.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TomFrost/jexl/issues"
  },
  "homepage": "https://github.com/TomFrost/jexl",
  "devDependencies": {
    "eslint": "^6.8.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
