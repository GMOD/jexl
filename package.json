{
  "name": "@jbrowse/jexl",
  "version": "3.0.1",
  "description": "A fork of the jexl lang for jbrowse",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "import": "./esm/index.js"
    },
    "require": {
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "esm",
    "src"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "clean": "rimraf dist esm",
    "build:esm": "tsc --outDir esm",
    "build:es5": "tsc --module commonjs --outDir dist",
    "build": "yarn build:esm && yarn build:es5",
    "prebuild": "yarn clean",
    "postbuild:es5": "echo '{\"type\": \"commonjs\"}' > dist/package.json",
    "preversion": "yarn lint && yarn test --run && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TomFrost/jexl"
  },
  "keywords": [
    "JSON",
    "expression",
    "evaluator",
    "parser",
    "target",
    "context",
    "jsep",
    "filter",
    "selector"
  ],
  "author": "Tom Shawver <tom@frosteddesign.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TomFrost/jexl/issues"
  },
  "homepage": "https://github.com/TomFrost/jexl",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vitest": "^4.0.16"
  },
  "publishConfig": {
    "access": "public"
  }
}
